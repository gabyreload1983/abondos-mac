<div class="container mt-3">
  <div class="row justify-content-center">
    <div class="col-12 col-md-6">
      <h1>Ingresar Terminal</h1>
      <div class="mb-3">
        <select class="form-select" id="listCustomers">
          <option selected>Elija Cliente</option>
        </select>
      </div>

      <div class="form-floating mb-3">
        <input
          required
          type="text"
          class="form-control"
          id="mac"
          placeholder="MAC"
          name="mac"
        />
        <label for="mac">MAC</label>
      </div>

      <div class="form-floating mb-3">
        <input
          type="text"
          class="form-control"
          id="code_terminal"
          name="code_terminal"
        />
        <label for="code_terminal">Numero Equipo - Opcional</label>
      </div>

      <div class="form-floating mb-3">
        <input
          type="text"
          class="form-control"
          id="description"
          name="description"
        />
        <label for="description">Descripcion - Opcional</label>
      </div>

      <button
        id="addingTerminal"
        class="btn btn-outline-primary"
      >Ingresar</button>
    </div>

  </div>
</div>
<script src="/js/terminals.js"></script>